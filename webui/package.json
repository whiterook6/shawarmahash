{
  "name": "webui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint src --fix",
      "prettier --write src"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src",
    "lint": "eslint src --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.51.0"
  }
}
